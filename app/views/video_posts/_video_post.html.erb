
<div>
<span><%= video_post.user.username %></span>
  <span><%= link_to video_post.video_title, video_post %> </span>
<span><%= video_post.video_description %> </span>
  <span><%= video_post.video_url %> </span>
  <% if signed_in? && current_user.admin? %>
      <%= link_to "delete", video_post, method: :delete, confirm: "You sure?" %>
  <% end %>
</div>

